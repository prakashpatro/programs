bool solution(string inputString) {
    char s[100000];
    //s=inputString;
    int count=0;
    long long int i, k;
    for (i=0; inputString[i]!='\0'; i++);
    k=i;
    for(i=0; i<k/2; i++)
    {
        if( inputString[i]!=inputString[k-i-1])
        {
        count++;
        break;
        }
    }
    if(count==0)
    return true;
    else return false;
}
