string solution(string s) {
int n, i, count=0;
string str;
n=s.size();
for(i=0; i<n-1; i++)
{
    if(s[i]==s[i+1])
    count++;
    
    else{
        count++;
        if(count==1)
        str=str+s[i];
        else
        str=str+to_string(count)+s[i];
        count=0;
    }
}
count++;
if(count==1)
     str=str+s[i];
    else
    str=str+to_string(count)+s[i];

return str;
}
