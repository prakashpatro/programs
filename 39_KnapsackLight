int solution(int value1, int weight1, int value2, int weight2, int maxW) {
int a=weight1+weight2;

if(a<=maxW)
return value1+value2;

if(weight1>maxW)
{
    if(weight2>maxW)
    return 0;
    
    else {
    return value2;
    }
}

if(weight2>maxW)
return value1;

else if(value2>value1)
return value2;

else return value1;
