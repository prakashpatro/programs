int solution(vector<vector<int>> matrix) {
int count=0, i, j, k, l=0, m, n, a[10000]={0};
n=matrix.size();
m=matrix[0].size();

for(i=0; i<n-1; i++)
{
    for(j=0; j<m-1; j++)
    {
        a[l]=a[l]*10 + matrix[i][j];
        a[l]=a[l]*10 + matrix[i][j+1];
        a[l]=a[l]*10 + matrix[i+1][j];
        a[l]=a[l]*10 + matrix[i+1][j+1];
        l++;
        
    }
}

set<int> s(a, a+l);
return s.size();
}