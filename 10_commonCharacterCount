int solution(string s1, string s2) {
int i, j, count=0, k, l;
k=s1.length();
l=s2.length();
for(i=0; i<k; i++)
{
    for(j=0; j<l; j++)
    {
        if(s1[i]==s2[j])
        {
            s2[j]='0';
            break;
        }
    }
}
for(j=0; j<l; j++)
{
    if(s2[j]=='0')
    count++;

}
return count;
}
