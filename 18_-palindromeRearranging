bool solution(string inputString) {
int n, i, j, count=0;
n=inputString.size();

for(i=0; i<n-1; i++)
{
    if(inputString[i]=='0')
    continue;
    count++;
    if(count>2)
    break;
   
    for(j=i+1; j<n ; j++)
    {
       if(inputString[i]==inputString[j])
       {
           inputString[j]='0';
           inputString[i]='0';
           count--;
           break;
       } 
    }
}

if(count==0)
return true;

else if(count==1 && n%2==1)
{
    return true;
}
else 
return false;
}
