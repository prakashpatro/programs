bool solution(vector<int> a, vector<int> b) {
    long long int count=0, n, i;
    n=a.size();
    
    for(i=0; i<n; i++)
    {
        if(a[i]!=b[i])
        {
            count++;
            if(count>2)
            return false;
        }
    }
    
    sort(a.begin(), a.begin()+n);
    sort(b.begin(), b.begin()+n);
    
    for(i=0; i<n; i++)
    {
        if(a[i]!=b[i])
        return false;
    }
     
     return true;

}
