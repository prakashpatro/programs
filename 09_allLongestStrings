vector<string> solution(vector<string> inputArray) {
int i, n, max=0;
vector<string> output;
n=inputArray.size();
for(i=0; i<n; i++)
{
    if(inputArray[i].size()>max)
    max=inputArray[i].size();
}
int j=0;
for(i=0; i<n; i++)
{
    if(inputArray[i].size()==max)
    {
        output.push_back(inputArray[i]);
    }
}
return output;
}
